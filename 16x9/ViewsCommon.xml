					<selectedcolor>selected</selectedcolor>
					<align>left</align>
					<aligny>center</aligny>
					<label>$VAR[MusicListLabel3Var]</label>
					<animation effect="slide" start="0,0" end="0,25" time="0" condition="IsEmpty(ListItem.Artist)">Conditional</animation>
